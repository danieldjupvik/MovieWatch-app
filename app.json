    },
    "android": {
      "package": "com.danieldjupvik.MovieWatcht",
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1D1D1C"
      },
